{
    "name": "brilliantsole-javascript-sdk",
    "version": "1.0.0",
    "description": "JavaScript SDK for BrilliantSole Smart Insoles",
    "main": "build/brilliantsole.cjs",
    "module": "./build/brilliantsole.module.js",
    "browser": "build/brilliantsole.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./build/brilliantsole.module.js"
        },
        "./src/*": "./src/*"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "server": "node server.cjs"
    },
    "repository": "https://github.com/zakaton/BrilliantSole.js.git",
    "homepage": "https://www.runonbrilliantsole.com/",
    "author": "Zack Qattan <zack@brilliantsole.com>",
    "license": "MIT",
    "private": true,
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@types/web-bluetooth": "^0.0.20",
        "express": "^4.18.3",
        "ip": "^2.0.1",
        "magic-string": "^0.30.5",
        "rollup": "^4.9.1",
        "ws": "^8.16.0"
    },
    "dependencies": {
        "@abandonware/noble": "^1.9.2-24",
        "webbluetooth": "^3.2.1"
    },
    "files": [
        "package.json",
        "README.md",
        "LICENSE",
        "src",
        "build",
        "examples"
    ],
    "directories": {
        "example": "examples"
    }
}
